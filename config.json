{
    "prefix": "!",
    "token": "PLACEHOLDER_TOKEN",
    "clientId": "PLACEHOLDER_CLIENT_ID", 
    "guildId": "PLACEHOLDER_GUILD_ID",
    "ownerId": "PLACEHOLDER_OWNER_ID",
    "embedColor": "#7289DA",
    "errorColor": "#FF0000",
    "successColor": "#00FF00",
    "warningColor": "#FFA500",
    "maxWarnings": 3,
    "defaultMuteTime": "1h",
    "maxPurgeAmount": 1000,
    "giveawayColor": "#FFD700",
    "logsRetentionDays": 30,
    "autoDeleteWarningsAfterDays": 90,
    "rateLimitWindow": 60000,
    "rateLimitMax": 10,
    "database": {
        "path": "./data/bot.db"
    },
    "permissions": {
        "admin": ["Administrator", "ManageGuild"],
        "moderator": ["ModerateMembers", "ManageMessages", "KickMembers", "BanMembers"],
        "helper": ["ManageMessages"]
    },
    "automod": {
        "enabled": true,
        "spamThreshold": 8,
        "mentionThreshold": 8,
        "capsThreshold": 80,
        "linkWhitelist": ["discord.gg", "youtube.com", "github.com", "twitter.com", "x.com", "reddit.com", "twitch.tv", "instagram.com", "tiktok.com", "spotify.com", "soundcloud.com", "wikipedia.org", "google.com", "imgur.com", "gyazo.com", "steamcommunity.com", "tenor.com", "giphy.com"]
    }
}
